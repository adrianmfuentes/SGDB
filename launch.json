{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Tests",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/sgdb_test",  
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/usr/bin/gdb",
        "preLaunchTask": "build sgdb",
        "logging": {
          "moduleLoad": false,
          "trace": false,
          "engineLogging": false
        }
      }
    ]
  }
  